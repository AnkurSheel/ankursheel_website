{
    "name": "ankursheel_website",
    "version": "1.0.0",
    "description": "Blog for Ankur Sheel",
    "keywords": [
        "blog"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/AnkurSheel/ankursheel_website"
    },
    "license": "MIT",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "scripts": {
        "echo": "echo \"Dummy command for netlify\"",
        "postinstall": "yarn deduplicate --list && yarn deduplicate",
        "build": "gatsby build",
        "develop": "gatsby develop",
        "clean": "gatsby clean",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx} config/**/*.js gatsby-*.js content/**/*.{md,mdx,markdown}",
        "generatePostPreviewImages": "ts-node ./scripts/generatePostPreviewImages.ts",
        "createBlogPost": "ts-node ./scripts/createNewBlogpost.ts",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
        "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "lint --fix",
        "lint:markdown": "markdownlint **/*.mdx --ignore node_modules/**",
        "lint:markdown:fix": "markdownlint **/*.mdx --ignore node_modules/** --fix",
        "generate-graphql": "graphql-codegen --config ./config/codegen.yml",
        "deduplicate": "yarn-deduplicate"
    },
    "dependencies": {
        "@codinators/gatsby-theme-blog": "^2.0.3",
        "@sindresorhus/slugify": "^0.10.0",
        "gatsby": "^2.18.12",
        "gatsby-plugin-google-tagmanager": "^2.1.20",
        "gatsby-plugin-manifest": "^2.2.40",
        "gatsby-plugin-offline": "^3.0.29",
        "inquirer": "^7.0.0",
        "json-to-pretty-yaml": "^1.2.2",
        "mkdirp": "^0.5.1",
        "prettier": "^1.18.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "ts-node": "^8.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@codinators/eslint-config": "^1.0.0",
        "@types/inquirer": "^6.5.0",
        "@types/prettier": "^1.18.2",
        "@types/puppeteer": "^2.0.0",
        "@types/react": "^16.9.17",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.12.0",
        "eslint": "^6.3.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^2.0.1",
        "gatsby-plugin-netlify": "^2.1.30",
        "glob": "^7.1.4",
        "husky": "^4.2.1",
        "lint-staged": "^10.0.7",
        "markdownlint-cli": "^0.21.0",
        "netlify-cli": "^2.25.0",
        "npm-normalize-package-bin": "^1.0.1",
        "puppeteer": "^2.0.0",
        "typescript": "^3.6.3",
        "webpack": "^4.41.4",
        "yalc": "^1.0.0-pre.34",
        "yarn-deduplicate": "^1.1.1"
    },
    "peerDependencies": {
        "gatsby": "^2.13.7",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
