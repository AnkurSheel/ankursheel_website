{
    "name": "ankursheel_website",
    "version": "1.0.0",
    "description": "Blog for Ankur Sheel",
    "keywords": [
        "blog"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/AnkurSheel/ankursheel_website"
    },
    "license": "MIT",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "clean": "gatsby clean",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx} config/gatsby/siteConfig.js gatsby-*.js content/**/*.{md,mdx,markdown}",
        "generatePostPreviewImages": "node ./scripts/generatePostPreviewImages.js",
        "createBlogPost": "ts-node ./scripts/createNewBlogpost.ts",
        "start": "gatsby develop",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
        "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "yarn lint --fix",
        "generate-graphql": "graphql-codegen --config ./config/codegen.yml"
    },
    "dependencies": {
        "@emotion/core": "^10.0.15",
